Algoritmo "questao04"
// Desenvolver um programa que apresente os resultados de uma tabuada de um número qualquer.
Var
   cont, tabuada, n: inteiro

Inicio
   escreva("Informe um valor: ")
   leia(n)
   para cont de 1 ate 10 passo 1 faca
      escreval(n, " *", cont, " =", n * cont)
   fimpara

Fimalgoritmo